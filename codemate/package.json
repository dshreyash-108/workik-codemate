{
  "name": "codemate",
  "displayName": "codemate",
  "description": "Ai assistant that helps user to write the code and debug it.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "codemate.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "codemate.Prompt",
        "title": "Enter your Prompt here: "
      },
      {
        "command": "codemate.selectcode",
        "title": "Your selected code: "
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.89.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@langchain/google-genai": "^0.0.14",
    "langchain": "^0.2.0"
  }
}
